    "firebaseui": "^1.0.0",
    "jss-theme-reactor": "^0.10.0",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-alpha.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
