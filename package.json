{
  "name": "immigrantgreensite",
  "config": {
    "presets": [
      "neutrino-preset-react",
      "neutrino-preset-jest"
    ],
    "neutrino": {
      "entry": {
        "vendor": [
          "react",
          "react-dom",
          "react-hot-loader",
          "react-router-dom"
